/v-divider>\n          <v-list-item-group class=\"mb-5\">\n            <v-list-item>\n              <v-list-item-title class=\"overline\">Frequent Activities</v-list-item-title>\n            </v-list-item>\n            <v-list-item to=\"/login\" v-if=\"!currentUser\">\n              <v-list-item-icon><v-icon small>fad fa-sign-in</v-icon></v-list-item-icon>\n              <v-list-item-content>Login</v-list-item-content>\n            </v-list-item>\n            <v-list-item v-else @click=\"logout\">\n              <v-list-item-icon><v-icon small>fad fa-sign-out</v-icon></v-list-item-icon>\n              <v-list-item-content>Log out</v-list-item-content>\n            </v-list-item>\n\n            <v-list-item to=\"/\">\n              <v-list-item-icon><v-icon small>fad fa-calendar</v-icon></v-list-item-icon>\n              <v-list-item-content>Schedule</v-list-item-content>\n            </v-list-item>\n            <v-list-item to=\"/standings\">\n              <v-list-item-icon><v-icon small>fas fa-trophy-alt</v-icon></v-list-item-icon>\n              <v-list-item-content>Points</v-list-item-content>\n            </v-list-item>\n            <v-divider></v-divider>\n            <v-list-item>\n              <v-list-item-title class=\"overline\">Learn More</v-list-item-title>\n            </v-list-item>\n            <v-list-item @click.stop=\"awardsDialog = !awardsDialog\">\n              <v-list-item-icon><v-icon small>fas fa-sack-dollar</v-icon></v-list-item-icon>\n              <v-list-item-content>Awards</v-list-item-content>\n            </v-list-item>\n            <v-list-item @click.stop=\"feesDialog = !feesDialog\">\n              <v-list-item-icon><v-icon small>fas fa-dollar-sign</v-icon></v-list-item-icon>\n              <v-list-item-content>Fees</v-list-item-content>\n            </v-list-item>\n            <v-list-item v-for=\"item in items\" v-bind:key=\"item['.key']\" :to=\"item.link\">\n              {{item.text}}\n            </v-list-item>\n          </v-list-item-group>        </v-list>\n      </v-navigation-drawer>\n      <v-app-bar\n        app\n        flat\n        >\n        <template v-slot:img=\"{ props }\">\n          <v-img\n            v-bind=\"props\"\n            gradient=\"270deg, rgba(224,67,54,.9), rgba(33,33,33,.7)\"\n            class=\"mr-3\"\n          ></v-img>\n        </template>\n  \n        <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\"></v-app-bar-nav-icon>\n        <!--<v-img\n          height=\"100\"\n          src=\"@/assets/logo.png\"\n          alt=\"Gopher State Garden Tractor Pullers logo\"\n          class=\"mx-2\"\n          contain\n         />-->\n\n         <span class=\"title mt-2\">Gopher State <span class=\"font-weight-light\">Garden Tractor Pullers</span></span>\n        <!--<v-toolbar-title>Gopher State Garden Tractor Pullers</v-toolbar-title>-->\n  \n        \n        \n      </v-app-bar>\n      \n      <v-content style=\"margin-bottom:50px !important;\"> \n        <v-container>\n          <router-view class=\"\"></router-view>\n        </v-container>\n     \n      </v-content>\n    <v-container v-if=\"admin == true\">\n    <v-speed-dial v-model=\"adminSpeedy\" fixed bottom right>\n      <template v-slot:activator>\n        <v-btn v-model=\"adminSpeedy\" color=\"green darken-2\" dark fab>\n      <v-icon v-if=\"adminSpeedy\">fad fa-lock-open</v-icon>\n      <v-icon v-else>fad fa-lock</v-icon>\n        </v-btn>\n      </template>\n      <v-btn fab dark small color=\"pink\" to=\"/addEvent\"><v-icon color=\"white\">fad fa-calendar-star</v-icon></v-btn>\n      <v-btn fab dark small color=\"indigo\" @click.stop=\"addMemberDialog = true\"><v-icon small>fad fa-user-plus</v-icon></v-btn>\n      <v-btn fab dark small color=\"blue\" @click.stop=\"addVehicleDialog = true\"><v-icon small>fad fa-tractor</v-icon></v-btn>\n      <v-btn fab dark small color=\"amber\" @click.stop=\"addHookDialog = true\"><v-icon small>fad fa-link</v-icon></v-btn>\n      </v-speed-dial>\n    </v-container>\n\n      <v-dialog v-model=\"addMemberDialog\" scrollable @update-dialog=\"updateDialog\">\n        <addMember style=\"background-color:white;\"></addMember>\n      </v-dialog>\n      <v-dialog v-model=\"addVehicleDialog\">\n        <addVehicle style=\"background-color:white;\"></addVehicle>\n      </v-dialog>\n      <v-dialog v-model=\"addHookDialog\">\n        <addHook style=\"background-color:white;\"></addHook>\n      </v-dialog>\n      <v-dialog v-model=\"awardsDialog\" :awardsDialog.sync=\"awardsDialog\" >\n        <awards style=\"background-color:white;\"></awards>\n      </v-dialog>\n  </v-app>\n\n    \n</template>\n\n<script>\nimport {auth, db, users} from '../firebaseconfig.js'\nimport {mapState} from 'vuex'\nimport addMember from './components/addMember.vue'\nimport addVehicle from './components/addVehicle.vue'\nimport addHook from './components/addHooks.vue'\nimport awards from './components/awards.vue'\n\nexport default {\n  name: 'App',\n\n  components: {\n    addMember,addVehicle,addHook, awards\n  },\n\n  data: () => ({\n    drawer: false,\n    addMemberDialog: false,\n    addVehicleDialog: false,\n    addHookDialog:false,\n    awardsDialog: false,\n    feesDialog: false,\n    adminSpeedy: false,\n    group: null,\n    currentYear: '2020',\n    items : [\n      \n      {icon: '', text: 'About Us', link: '/about'},\n      {icon: '', text: 'Rules', link: '/rules'},\n      {icon: '', text: 'Classes', link: '/vehicleClasses'},\n      {icon: '', text: 'Host Cities', link: '/host'},\n      \n    ]\n    //\n  }),\n  methods: {\n    logout() {\n      auth.signOut().then(() => {\n        this.$store.dispatch('clearData')\n        this.$router.push('/')\n      }).catch(err => {\n        console.log(err)\n      })\n    },\n    updateDialog(e) {\n      awardsDialog = !awardsDialog\n    }\n  },\n  watch: {\n    group () {\n      this.drawer = false\n    }\n  },\n  computed: {\n    ...mapState(['currentUser', 'userProfile', 'userRole']),\n    admin: function() { \n      let admin \n      if (this.$store.state.userProfile.role == 'admin') {return admin = true} else {return admin = false}\n      return admin\n      }\n  },\n  mounted() {\n    let fontawesomescript = document.createElement('script')\n    fontawesomescript.setAttribute('src', 'https://kit.fontawesome.com/be2cd14556.js')\n    document.head.appendChild(fontawesomescript)\n   \n  }\n};\n</script>\n\n<style>\na {\n  color: #AF372F !important}\n\nbody::-webkit-scrollbar {\n  width: 0.5em;\n}\n \nbody::-webkit-scrollbar-track {\n  box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);\n}\n \nbody::-webkit-scrollbar-thumb {\n  background-color:red;\n  outline: 1px solid black;\n}\n\n</style>"]}]}