,KAAA,WAAA,CAAA;AAfA,SAAA,EAgBA,IAhBA,CAgBA,UAAA,MAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,wBAAA,EAAA,MAAA,CAAA,EAAA;AACA,UAAA,EAAA,CAAA,cAAA,GAAA,6BAAA;AACA,UAAA,EAAA,CAAA,UAAA,GAAA,EAAA,EACA,EAAA,CAAA,QAAA,GAAA,KADA,EAEA,EAAA,CAAA,YAAA,GAAA,EAFA;AAGA,UAAA,EAAA,CAAA,MAAA,CAAA,OAAA,GAAA,EAAA,EACA,EAAA,CAAA,SAAA,CAAA,OAAA,GAAA,EADA;AAGA,SAzBA,EAyBA,KAzBA,CAyBA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,YAAA,GAAA,GAAA,CAAA,OAAA;AACA,SA3BA;AA4BA;AAEA,KAnIA;AAoIA,IAAA,OApIA,mBAoIA,CApIA,EAoIA;AACA,MAAA,CAAA,GAAA,EAAA;AACA;AAtIA;AAnEA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n         <v-card :loading=\"performingRequest\">\r\n         <v-container>\r\n           <v-card-title>\r\n           Add Hook\r\n           </v-card-title>\r\n           <v-alert v-if=\"!currentUser\" color=\"error\" dark>  \r\n             You must be signed in to add a hook / register for a pull.\r\n           </v-alert>\r\n           <v-card-text>\r\n             <v-form>\r\n               <!-- Find a pull -->\r\n               <v-autocomplete\r\n                v-model=\"hookForm.pull\"\r\n                :items=\"pulls\"\r\n                item-text=\"date\"\r\n                item-value=\"id\"\r\n                :value='dateRef'\r\n                return-object\r\n                color=\"info\"\r\n                label=\"Pull Date\"\r\n                clearable\r\n                chips\r\n                auto-select-first\r\n                item-color=\"info\"\r\n                >\r\n                <template v-slot:selection=\"data\">\r\n                  <v-chip\r\n                    v-bind=\"data.attrs\"\r\n                    :input-value=\"data.selected\"\r\n                    close\r\n                    @click=\"data.select\"\r\n                    color=\"info\"\r\n                  >\r\n                    \r\n                    {{ data.item.date }}\r\n                  </v-chip>\r\n                </template>\r\n               </v-autocomplete>\r\n               <div v-if=\"hookForm.pull\" style=\"margin-top:-15px\">\r\n                <strong>{{hookForm.pull.title}}</strong>, {{hookForm.pull.city}}, {{hookForm.pull.state}}\r\n                </div> \r\n               <!-- Determine if hook is for a member -->\r\n               <v-switch\r\n                v-model=\"hookMember\"\r\n                label=\"Member\"\r\n                inset\r\n                color=\"info\"\r\n               ></v-switch>\r\n               <div v-if=\"hookMember\">\r\n               <v-autocomplete\r\n                v-model=\"hookForm.member.selectedMember\"\r\n                label=\"Select member\"\r\n                :items=\"members\"\r\n                hint=\"Search for primary member associated to tractor.\"\r\n                item-text=\"fullName\"\r\n                item-value=\"userId\"\r\n                return-object\r\n                chips\r\n                deletable-chips\r\n               >\r\n               </v-autocomplete>\r\n               <v-autocomplete\r\n                v-if=\"hookForm.member.selectedMember\"\r\n                v-model=\"hookForm.member.vehicle\"\r\n                label=\"Find vehicle\"\r\n                :items=\"vehicles\"\r\n                hint=\"Select a vehicle.\"\r\n                item-text=\"name\"\r\n                item-value=\"id\"\r\n                return-object\r\n                chips\r\n                deletable-chips\r\n               >\r\n               </v-autocomplete>\r\n               \r\n               <v-autocomplete\r\n                v-model=\"hookForm.vehicleClass\"\r\n                label=\"Select vehicle class\"\r\n                :items=\"hookForm.member.vehicle.classes\"\r\n                hint=\"Select a vehicle class\"\r\n                item-text=\"name\"\r\n                item-value=\"name\"\r\n                v-if=\"hookForm.member.vehicle\"\r\n                multiple\r\n                return-object\r\n                chips\r\n                deletable-chips\r\n               >\r\n               </v-autocomplete>\r\n               <v-text-field v-model=\"hookForm.pullerName\" label=\"Puller Name (Optional)\"></v-text-field>\r\n                <v-switch\r\n                v-model=\"duesStatus\"\r\n                inset\r\n                color=\"info\"\r\n                label=\"Dues Paid\"\r\n                disabled\r\n                >\r\n                </v-switch>\r\n               </div>\r\n               <div v-if=\"!hookMember\">\r\n                 <v-text-field v-model=\"hookForm.pullerName\" label=\"Puller Name\" hint=\"Required for all guest hooks.\"></v-text-field>\r\n                 <v-text-field v-model=\"hookForm.nonMember.vehicle\" label=\"Vehicle\" hint=\"Provide a tractor name.\"></v-text-field>\r\n                 <v-autocomplete\r\n                v-model=\"hookForm.vehicleClass\"\r\n                label=\"Select vehicle class\"\r\n                :items=\"vehicleClasses\"\r\n                hint=\"Select a vehicle class\"\r\n                item-text=\"name\"\r\n                item-value=\"name\"\r\n                v-if=\"hookForm.nonMember.vehicle\"\r\n                multiple\r\n                return-object\r\n                chips\r\n                deletable-chips\r\n               >\r\n               </v-autocomplete>\r\n               </div>\r\n               <v-switch\r\n                v-model=\"hookFees\"\r\n                inset\r\n                color=\"info\"\r\n                label=\"Hook Fees\"\r\n                >\r\n                </v-switch>\r\n                <v-card-actions>\r\n                  <v-spacer></v-spacer>\r\n                  <v-btn text>Cancel</v-btn>\r\n                  <v-btn outlined color=\"success\" v-if=\"hookMember\" @click.stop=\"addMemberHook\">Add member hook</v-btn>\r\n                  <v-btn outlined color=\"success\" v-if=\"!hookMember\" @click.stop=\"addGuestHook\">Add guest hook</v-btn>\r\n                </v-card-actions>\r\n             </v-form>\r\n           </v-card-text>\r\n         </v-container>\r\n           </v-card>\r\n     \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {db} from '../../firebaseconfig.js'\r\nimport { mapState } from 'vuex'\r\nexport default {\r\n  name: 'addHooks',\r\n  props: ['dateRef', 'memberId'],\r\n  data() {\r\n    return {\r\n      pulls: {},\r\n      vehicleClasses: {},\r\n      vehicles: {},\r\n      hook: {},\r\n      performingRequest: false,\r\n      btmHook: false,\r\n      hookMember: false,\r\n      hookFees: false,\r\n      hookDues: '',\r\n      hookForm: {\r\n        successMessage: '',\r\n        errorMessage: '',\r\n        nonMember: {\r\n          vehicle: '',\r\n        },\r\n        member: {\r\n          vehicle: '',\r\n          selectedMember: '',\r\n        },\r\n        distance: '',\r\n        dq: false,\r\n        pull: '',\r\n        pullerName: '',\r\n        vehicleClass: ''\r\n      }\r\n    }\r\n  },\r\n  firestore() {\r\n    \r\n    return {\r\n      \r\n      //pull: db.collection('pulls').doc(currentPull),\r\n      hook: db.collection('hooks').orderBy(\"created\", \"desc\").limit(1),\r\n      members: db.collection('members'),\r\n      vehicles: db.collection('vehicles'),\r\n      vehicleClasses: db.collection('vehicleClasses').orderBy(\"name\", \"asc\"),\r\n      pulls: db.collection('pulls').where(\"track\", \"in\", [\"Tar\", \"Dirt\"]).orderBy('date', 'desc')\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState(['currentUser']),\r\n    currentUserVehicle(user) {\r\n       _.filter(vehicles, {'userId': user} )\r\n    },\r\n    duesPaid() {\r\n      if (this.hookForm.member.selectedMember != '') {\r\n      let mR = this.hookForm.member.selectedMember\r\n        let cD = mR.currentDues\r\n        let effEnd = cD.effectiveEnd.seconds\r\n        if (mR != null && cD != null) {\r\n            let utcDate = new Date(effEnd * 1000)\r\n            return utcDate   \r\n        }}\r\n    },\r\n    duesStatus() {\r\n      let hD = new Date(this.hookForm.pull.date)\r\n      if (hD != null && this.duesPaid != null) {\r\n        return this.duesPaid >= hD\r\n      }\r\n      \r\n    }\r\n  },\r\n    methods: {\r\n      addMemberHook: function() {\r\n        this.performingRequest = true\r\n        let form = document.getElementById(\"addHookForm\")\r\n        let vn = this.hookForm.member.vehicle.name\r\n        let hf = this.hookForm\r\n        let dS = this.duesStatus\r\n        let pullYear = this.hookForm.pull.date.substring(0,4)\r\n        console.log('dS:'+ dS)\r\n        \r\n        // Begin class eval (create multiple hooks)\r\n        let eachClass = this.hookForm.vehicleClass\r\n          for(let i=0, len=eachClass.length; i<len; i++) {  \r\n            let thisClass = eachClass[i].name\r\n\r\n            db.collection('hooks').add({\r\n                vehicle: this.hookForm.member.vehicle.name,\r\n                vehicleId: this.hookForm.member.vehicle.id,\r\n                pull: this.hookForm.pull,\r\n                pullRef: this.hookForm.pull.cityRef,\r\n                pullId: this.hookForm.pull.id,\r\n                pullerName: this.hookForm.pullerName,\r\n                track: this.hookForm.pull.track,\r\n                isMember: this.hookMember,\r\n                member: this.hookForm.member.selectedMember,\r\n                duesPaid: dS,\r\n                dq: this.hookForm.dq,\r\n                vehicleClass: thisClass,\r\n                pullOrder: (Math.random() * 100),\r\n                pointsYear: pullYear,\r\n                created: new Date(),\r\n                createdBy: this.currentUser.uid\r\n            }).then(function(docRef) {\r\n                console.log(\"Hook written with ID: \", docRef.id)\r\n                hf.successMessage = \"Hook(s) added successfully!\"\r\n                hf.pullerName = '',\r\n                hf.isMember = false,\r\n                hf.vehicleClass = ''\r\n                hf.member.vehicle = '',\r\n                hf.nonMember.vehicle = ''\r\n                hf.member.selectedMember = '',\r\n                hf.hookFees = false  \r\n                this.performingRequest = false\r\n            }).catch(err => {\r\n              this.hookForm.errorMessage = err.message\r\n              this.performingRequest = false\r\n            }); \r\n            }\r\n        },\r\n        addGuestHook: function() {\r\n        let form = document.getElementById(\"addHookForm\")\r\n        let vn = this.vehicleName\r\n        let hf = this.hookForm\r\n        let pullYear = this.hookForm.pull.date.substring(0,4)\r\n        \r\n        // Begin class eval (create multiple hooks)\r\n        let eachClass = this.hookForm.vehicleClass\r\n          for(let i=0, len=eachClass.length; i<len; i++) {  \r\n            let thisClass = eachClass[i].name\r\n\r\n            db.collection('hooks').add({\r\n                vehicle: this.hookForm.nonMember.vehicle,\r\n                pull: this.hookForm.pull.date,\r\n                pullRef: this.hookForm.pull.cityRef,\r\n                pullId: this.hookForm.pull.id,\r\n                pullerName: this.hookForm.pullerName,\r\n                track: this.hookForm.pull.track,\r\n                dq: this.hookForm.dq,\r\n                vehicleClass: thisClass,\r\n                pullOrder: (Math.random() * 100),\r\n                created: new Date(),\r\n                createdBy: this.currentUser.uid\r\n            }).then(function(docRef) {\r\n                console.log(\"Hook written with ID: \", docRef.id)\r\n                hf.successMessage = \"Hook(s) added successfully!\"\r\n                hf.pullerName = '',\r\n                hf.isMember = false,\r\n                hf.vehicleClass = ''\r\n                hf.member.vehicle = '',\r\n                hf.nonMember.vehicle = ''  \r\n            }).catch(err => {\r\n              this.hookForm.errorMessage = err.message\r\n            }); \r\n            }\r\n        },\r\n      addHook: function() {\r\n            // start transition/loading view\r\n           \r\n            let form = document.getElementById(\"addHookForm\")\r\n            let vn = this.vehicleName\r\n            let hf = this.hookForm\r\n            //let doc = this.vehicleForm.name.toLowerCase().split(\" \").join(\"-\")\r\n\r\n            // add hook to Firestore\r\n            let pullYear = this.hookForm.pull.date.substring(0,4)\r\n            let eachClass = this.hookForm.vehicleClass\r\n\r\n            for(let i=0, len=eachClass.length; i<len; i++) {\r\n            \r\n            let thisClass = eachClass[i].name\r\n            \r\n            db.collection(\"hooks\").add({\r\n                vehicleName: vn,\r\n                vehicleId: this.hookForm.member.vehicle.id,\r\n                pull: this.hookForm.pull.date,\r\n                pullRef: this.hookForm.pull.cityRef,\r\n                pullId: this.hookForm.pull.id,\r\n                pullerName: this.hookForm.pullerName,\r\n                track: this.hookForm.pull.track,\r\n                isMember: this.hookMember,\r\n                duesPaid: this.hookDues,\r\n                dq: this.hookForm.dq,\r\n                vehicleClass: thisClass,\r\n                pullOrder: (Math.random() * 100),\r\n                pointsYear: pullYear,\r\n                created: new Date(),\r\n                createdBy: this.currentUser.uid\r\n            }).then(function(docRef) {\r\n                console.log(\"Hook written with ID: \", docRef.id)\r\n                hf.successMessage = \"Hook(s) added successfully!\"\r\n                hf.pullerName = '',\r\n                hf.isMember = false,\r\n                hf.vehicleClass = ''\r\n                hf.member.vehicle = '',\r\n                hf.nonMember.vehicle = ''\r\n               \r\n            }).catch(err => {\r\n              this.hookForm.errorMessage = err.message\r\n            });\r\n            }\r\n            \r\n      },\r\n      dismiss(m) {\r\n        m = \"\"\r\n      }\r\n    }\r\n  }\r\n\r\n</script>"],"sourceRoot":"src/components"}]}