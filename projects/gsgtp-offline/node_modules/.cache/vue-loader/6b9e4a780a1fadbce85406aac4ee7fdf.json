s=\"grey lighten-4\">\n          <v-img contains src=\"@/assets/logo.png\" contain height=\"150px\" to=\"/\"/>\n          \n          <v-list-item-group class=\"\">\n            <v-list-item v-for=\"item in items\" v-bind:key=\"item['.key']\" :to=\"item.link\">\n              {{item.text}}\n            </v-list-item>\n          </v-list-item-group>        </v-list>\n      </v-navigation-drawer>\n      <v-app-bar\n        app\n        dark\n        color=\"red\"\n        shrink-on-scroll\n        src=\"@/assets/MVIMG_20180818_160004.jpg\"\n        fade-img-on-scroll\n        >\n        <template v-slot:img=\"{ props }\">\n          <v-img\n            v-bind=\"props\"\n            gradient=\"270deg, rgba(224,67,54,.9), rgba(33,33,33,.7)\"\n            class=\"mr-3\"\n          ></v-img>\n        </template>\n  \n        <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\"></v-app-bar-nav-icon>\n        <!--<v-img\n          height=\"100\"\n          src=\"@/assets/logo.png\"\n          alt=\"Gopher State Garden Tractor Pullers logo\"\n          class=\"mx-2\"\n          contain\n         />-->\n\n         <span class=\"title mt-2\">Gopher State <span class=\"font-weight-light\">Garden Tractor Pullers</span></span>\n        <!--<v-toolbar-title>Gopher State Garden Tractor Pullers</v-toolbar-title>-->\n  \n        \n        \n      </v-app-bar>\n      \n      <v-content style=\"margin-bottom:50px !important;\"> \n        <v-container>\n          <router-view class=\"\"></router-view>\n        </v-container>\n     \n      </v-content>\n      \n      \n   <v-bottom-navigation color=\"red\" horizontal hide-on-scroll fixed>\n     <v-btn to=\"/login\" v-if=\"!currentUser\">Login</v-btn>\n     <v-btn @click=\"logout\" v-if=\"currentUser\">Log out</v-btn>\n     <v-btn to=\"/\">Schedule</v-btn>\n     <v-btn to=\"/standings\">Points</v-btn>\n     <v-btn to=\"/classes\">Classes</v-btn>\n     \n   </v-bottom-navigation>\n\n    <v-speed-dial v-model=\"adminSpeedy\" fixed bottom right v-if=\"role = admin\">\n      <template v-slot:activator>\n        <v-btn v-model=\"adminSpeedy\" color=\"green darken-2\" dark fab>\n      <v-icon v-if=\"adminSpeedy\">fad fa-lock-open</v-icon>\n      <v-icon v-else>fad fa-lock</v-icon>\n        </v-btn>\n      </template>\n      <v-btn fab dark small color=\"indigo\" @click.stop=\"addMemberDialog = true\"><v-icon small>fad fa-user-plus</v-icon></v-btn>\n      <v-btn fab dark small color=\"blue\" @click.stop=\"addVehicleDialog = true\"><v-icon small>fad fa-tractor</v-icon></v-btn>\n      <v-btn fab dark small color=\"green\" @click.stop=\"addHookDialog = true\"><v-icon small>fad fa-link</v-icon></v-btn>\n      </v-speed-dial>\n\n      <v-dialog v-model=\"addMemberDialog\">\n        <addMember style=\"background-color:white;\"></addMember>\n      </v-dialog>\n      <v-dialog v-model=\"addVehicleDialog\">\n        <addVehicle style=\"background-color:white;\"></addVehicle>\n      </v-dialog>\n      <v-dialog v-model=\"addHookDialog\">\n        <addHook style=\"background-color:white;\"></addHook>\n      </v-dialog>\n  </v-app>\n\n    \n</template>\n\n<script>\nimport {auth, db, users} from '../firebaseconfig.js'\nimport {mapState} from 'vuex'\nimport addMember from './components/addMember.vue'\nimport addVehicle from './components/addVehicle.vue'\nimport addHook from './components/addHooks.vue'\n\nexport default {\n  name: 'App',\n\n  components: {\n    addMember,addVehicle,addHook\n  },\n\n  data: () => ({\n    drawer: false,\n    addMemberDialog: false,\n    addVehicleDialog: false,\n    addHookDialog:false,\n    adminSpeedy: false,\n    group: null,\n    currentYear: '2020',\n    items : [\n      {icon: '', text: 'Home', link: '/'},\n      {icon: '', text: 'Host Cities', link: '/host'},\n      {icon: '', text: 'About Us', link: '/about'},\n      {icon: '', text: 'Rules', link: '/rules'},\n      {icon: '', text: 'Classes', link: '/vehicleClasses'},\n    ]\n    //\n  }),\n  methods: {\n    logout() {\n      auth.signOut().then(() => {\n        this.$store.dispatch('clearData')\n        this.$router.push('/')\n      }).catch(err => {\n        console.log(err)\n      })\n    }\n  },\n  watch: {\n    group () {\n      this.drawer = false\n    }\n  },\n  computed: {\n    ...mapState(['currentUser', 'userProfile']),\n    role() {\n     return this.userProfile.role\n    }\n  },\n  mounted() {\n    let fontawesomescript = document.createElement('script')\n    fontawesomescript.setAttribute('src', 'https://kit.fontawesome.com/be2cd14556.js')\n    document.head.appendChild(fontawesomescript)\n   \n  }\n};\n</script>\n\n<style>\na {\n  color: #AF372F !important}\n\n</style>"]}]}