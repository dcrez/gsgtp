 v-if=\"pull.track == 'N/A'\" color=\"gray lighten-2\" class=\"\">fas fa-do-not-enter</v-icon>\r\n            <v-icon v-if=\"pull.track == 'Tar'\" >fas fa-road</v-icon>\r\n            <v-icon v-if=\"pull.track == 'Dirt'\" >fas fa-digging</v-icon>\r\n          </v-list-item-icon>\r\n          <v-list-item-content v-if=\"!currentDate(pull)\">\r\n            <v-list-item-title>{{pull.city}}, {{pull.state}} <span v-if=\"pull.title\">- {{pull.title}}</span></v-list-item-title>\r\n            <v-list-item-subtitle class=\"text--primary\">{{pull.date | moment(\"dddd, MMMM Do YYYY\")}} at {{pull.time}} </v-list-item-subtitle>\r\n            <v-list-item-subtitle v-if=\"pull.description\">{{pull.description}}</v-list-item-subtitle>\r\n          </v-list-item-content>\r\n          <v-list-item-content v-else>\r\n            <v-list-item-title @click=\"pullDetails(pull)\">{{pull.city}}, {{pull.state}} <span v-if=\"pull.title\">- {{pull.title}}</span></v-list-item-title>\r\n            <v-list-item-subtitle class=\"text--primary\">{{pull.date | moment(\"dddd, MMMM Do YYYY\")}} at {{pull.time}} | {{pull.date | moment(\"from\", \"now\")}}</v-list-item-subtitle>\r\n            <v-list-item-subtitle v-if=\"pull.description\">{{pull.description}}</v-list-item-subtitle>\r\n          \r\n          </v-list-item-content>\r\n          <v-list-item-action>\r\n            {{pull.date | moment(\"from\", \"now\")}}\r\n          </v-list-item-action>\r\n          \r\n        </v-list-item>\r\n      </v-list-item-group>\r\n    </v-list>\r\n    </v-card>\r\n  </div>\r\n</template>\r\n<script>\r\nimport {db} from '../../firebaseconfig.js'\r\nexport default {\r\n  name: 'schedule',\r\n  data() {\r\n    return {\r\n      pulls: {}\r\n    }\r\n  },\r\n  firestore() {\r\n    return {\r\n      pulls: db.collection('pulls').orderBy(\"date\", \"desc\").limit(10)\r\n    }\r\n  },\r\n    methods: {\r\n      deletePull (pull) {\r\n           db.collection('pulls').doc(pull.id).delete();\r\n       },\r\n       pullDetails (pull) {\r\n         this.$router.push({path: `/pull/${pull.city}/${pull.id}`}) \r\n       },\r\n      currentDate(pull) {\r\n        let current = new Date()\r\n        return this.$moment(pull.date).isBefore(current)\r\n      }\r\n    }\r\n  }\r\n\r\n</script>"]}]}