d-text>\r\n        \r\n    </v-card>\r\n    <v-toolbar class=\"mt-5\" color=\"red darken-4\" dark>Hooks\r\n    </v-toolbar>\r\n    <v-card>\r\n\r\n      <v-data-table :headers=\"headers\" :items=\"hooks\" :sort-by=\"['vehicleClass', 'distance']\" :sort-desc=\"[false,true]\">\r\n      </v-data-table>\r\n    </v-card>\r\n    </div>\r\n        \r\n    \r\n \r\n</template>\r\n\r\n<script>\r\nimport {db} from '../../firebaseconfig.js'\r\nexport default {\r\n  name: 'pullDetails',\r\n  data() {\r\n    return {\r\n      pull: {},\r\n      hooks: {},\r\n      headers: [{\r\n        text: 'Class',\r\n        value: 'vehicleClass'\r\n      },\r\n      {\r\n        text: 'Name',\r\n        value: 'vehicleName'\r\n      },\r\n      {\r\n        text: 'Puller',\r\n        value: 'pullerName'\r\n      },\r\n      {\r\n        text: 'Distance',\r\n        value: 'distance'\r\n      },\r\n      {\r\n        text: 'DQ?',\r\n        value: 'dq'\r\n      },\r\n      {\r\n        text: 'Points',\r\n        value: 'points'\r\n      }]\r\n    }\r\n  },\r\n  firestore() {\r\n    let currentPull = this.$route.params.id\r\n    //let pullDate = this.currentPull.date\r\n    return {\r\n      pull: db.collection('pulls').doc(currentPull),\r\n      hooks: db.collection('hooks').where(\"pullId\", \"==\", currentPull).orderBy(\"vehicleClass\", \"asc\")\r\n    }\r\n  },\r\n    methods: {\r\n    save () {\r\n      this.snack = true\r\n      this.snackColor = 'success'\r\n      this.snackText = 'Data saved'\r\n    },\r\n    cancel () {\r\n      this.snack = true\r\n      this.snackColor = 'error'\r\n      this.snackText = 'Canceled'\r\n    },\r\n    open () {\r\n      this.snack = true\r\n      this.snackColor = 'info'\r\n      this.snackText = 'Dialog opened'\r\n    },\r\n    close () {\r\n      console.log('Dialog closed')\r\n    },\r\n  },\r\n  }\r\n\r\n</script>"]}]}