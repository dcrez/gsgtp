AASA,MAAA,WAAA,EAAA,MATA;AAUA,MAAA,KAAA,EAAA,CAEA;AAAA,QAAA,IAAA,EAAA,EAAA;AAAA,QAAA,IAAA,EAAA,UAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAFA,EAGA;AAAA,QAAA,IAAA,EAAA,EAAA;AAAA,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAHA,EAIA;AAAA,QAAA,IAAA,EAAA,EAAA;AAAA,QAAA,IAAA,EAAA,aAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAJA,CAVA,CAiBA;;AAjBA,KAAA;AAAA,GAPA;AA0BA,EAAA,SA1BA,uBA0BA;AACA,WAAA,CACA;AADA,KAAA;AAGA,GA9BA;AA+BA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AAAA;;AACA,MAAA,IAAA,CAAA,OAAA,GAAA,IAAA,CAAA,YAAA;AACA,QAAA,QAAA,CAAA,MAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,mCAAA;;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,WAAA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,cAAA;AACA,OALA,EAKA,KALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAPA;AAQA,KAVA;AAWA,IAAA,YAXA,wBAWA,CAXA,EAWA;AACA,MAAA,YAAA,GAAA,CAAA,YAAA;AACA;AAbA,GA/BA;AA8CA,EAAA,KAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,WAAA,MAAA,GAAA,KAAA;AACA;AAHA,GA9CA;AAmDA,EAAA,QAAA,kCACA,QAAA,CAAA,CAAA,aAAA,EAAA,aAAA,EAAA,UAAA,CAAA,CADA;AAEA,IAAA,KAAA,EAAA,iBAAA;AACA,UAAA,KAAA;;AACA,UAAA,KAAA,MAAA,CAAA,KAAA,CAAA,WAAA,CAAA,IAAA,IAAA,OAAA,EAAA;AAAA,eAAA,KAAA,GAAA,IAAA;AAAA,OAAA,MAAA;AAAA,eAAA,KAAA,GAAA,KAAA;AAAA;;AACA,aAAA,KAAA;AACA;AANA,IAnDA;AA2DA,EAAA,OA3DA,qBA2DA;AACA,QAAA,iBAAA,GAAA,QAAA,CAAA,aAAA,CAAA,QAAA,CAAA;AACA,IAAA,iBAAA,CAAA,YAAA,CAAA,KAAA,EAAA,2CAAA;AACA,IAAA,QAAA,CAAA,IAAA,CAAA,WAAA,CAAA,iBAAA;AACA;AA/DA,CAAA","sourcesContent":["<template>\n    <v-app id=\"app\" style=\"background-color:white;\">\n      <v-navigation-drawer v-model=\"drawer\" fixed temporary class=\"no-print\" width=\"350\">\n        <v-list class=\"\">\n          <v-list-item to=\"/\" >\n          <v-img contains src=\"@/assets/logo.png\" contain height=\"150px\" />\n          </v-list-item>\n          \n          <v-divider class=\"mt-3 mb-3\"></v-divider>\n          <v-list-item-group class=\"mb-5\">\n            <v-list-item>\n              <v-list-item-title class=\"overline\">Frequent Activities</v-list-item-title>\n            </v-list-item>\n            <v-list-item to=\"/login\" v-if=\"!currentUser\">\n              <v-list-item-icon><v-icon small>fad fa-sign-in</v-icon></v-list-item-icon>\n              <v-list-item-content>Login or Create Profile</v-list-item-content>\n            </v-list-item>\n            <v-list-item v-else @click=\"logout\">\n              <v-list-item-icon><v-icon small>fad fa-sign-out</v-icon></v-list-item-icon>\n              <v-list-item-content>Log out</v-list-item-content>\n            </v-list-item>\n\n            <v-list-item to=\"/\">\n              <v-list-item-icon><v-icon small>fad fa-calendar</v-icon></v-list-item-icon>\n              <v-list-item-content>Schedule</v-list-item-content>\n            </v-list-item>\n            <v-list-item to=\"/standings\">\n              <v-list-item-icon><v-icon small>fas fa-trophy-alt</v-icon></v-list-item-icon>\n              <v-list-item-content>Points</v-list-item-content>\n            </v-list-item>\n            <v-divider></v-divider>\n            <v-list-item>\n              <v-list-item-title class=\"overline\">Learn More</v-list-item-title>\n            </v-list-item>\n            <v-list-item href=\"/gsgtp-rule-book-2020.pdf\" target=\"_blank\">\n              <v-list-item-icon><v-icon small>fad fa-ruler-triangle</v-icon></v-list-item-icon>\n              <v-list-item-content>Rules</v-list-item-content>\n            </v-list-item>\n            <v-list-item to=\"/about\">\n              <v-list-item-icon><v-icon small>fas fa-address-card</v-icon></v-list-item-icon>\n              <v-list-item-content>About Us</v-list-item-content>\n            </v-list-item>\n           \n            <v-list-item to=\"/classes\">\n              <v-list-item-icon><v-icon small>fad fa-tractor</v-icon></v-list-item-icon>\n              <v-list-item-content>Classes</v-list-item-content>\n            </v-list-item>\n            \n            <v-list-item @click.stop=\"awardsDialog = !awardsDialog\">\n              <v-list-item-icon><v-icon small>fas fa-sack-dollar</v-icon></v-list-item-icon>\n              <v-list-item-content>Awards</v-list-item-content>\n            </v-list-item>\n            <v-list-item @click.stop=\"feesDialog = !feesDialog\" disabled>\n              <v-list-item-icon><v-icon small>fas fa-dollar-sign</v-icon></v-list-item-icon>\n              <v-list-item-content>Fees</v-list-item-content>\n            </v-list-item>\n            <v-list-item to=\"/host\">\n              <v-list-item-icon><v-icon small>fad fa-car-building</v-icon></v-list-item-icon>\n              <v-list-item-content>Host Cities</v-list-item-content>\n            </v-list-item>\n          </v-list-item-group>        </v-list>\n      </v-navigation-drawer>\n      <v-app-bar\n        app\n        flat\n        class=\"no-print\"\n        >\n  \n        <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\"></v-app-bar-nav-icon>\n        <!--<v-img\n          height=\"100\"\n          src=\"@/assets/logo.png\"\n          alt=\"Gopher State Garden Tractor Pullers logo\"\n          class=\"mx-2\"\n          contain\n         />-->\n\n         <span class=\"title mt-2\">Gopher State <span class=\"font-weight-light\">Garden Tractor Pullers</span></span>\n        <!--<v-toolbar-title>Gopher State Garden Tractor Pullers</v-toolbar-title>-->\n  \n        \n        \n      </v-app-bar>\n      \n      <v-content style=\"margin-bottom:50px !important;\"> \n        <v-container>\n          <router-view class=\"\"></router-view>\n        </v-container>\n     \n      </v-content>\n    <v-container v-if=\"admin == true\">\n    <v-speed-dial v-model=\"adminSpeedy\" fixed bottom right>\n      <template v-slot:activator>\n        <v-btn v-model=\"adminSpeedy\" color=\"green darken-2\" dark fab>\n      <v-icon v-if=\"adminSpeedy\">fad fa-lock-open</v-icon>\n      <v-icon v-else>fad fa-lock</v-icon>\n        </v-btn>\n      </template>\n      <v-btn fab dark small color=\"pink\" to=\"/addEvent\"><v-icon color=\"white\">fad fa-calendar-star</v-icon></v-btn>\n      <v-btn fab dark small color=\"indigo\" to=\"/member/add\"><v-icon small color=\"white\">fad fa-user-plus</v-icon></v-btn>\n      <v-btn fab dark small color=\"blue\" @click.stop=\"addVehicleDialog = true\"><v-icon small>fad fa-tractor</v-icon></v-btn>\n      <v-btn fab dark small color=\"amber\" @click.stop=\"addHookDialog = true\"><v-icon small>fad fa-link</v-icon></v-btn>\n      <v-btn fab dark small color=\"deep-orange\" to=\"/members\"><v-icon color=\"white\">fad fa-money-check-alt</v-icon></v-btn>\n      </v-speed-dial>\n    </v-container>\n\n     \n      <v-dialog v-model=\"addVehicleDialog\">\n        <addVehicle style=\"background-color:white;\"></addVehicle>\n      </v-dialog>\n      <v-dialog v-model=\"addHookDialog\">\n        <addHook style=\"background-color:white;\"></addHook>\n      </v-dialog>\n      <v-dialog v-model=\"awardsDialog\" :awardsDialog.sync=\"awardsDialog\" >\n        <awards style=\"background-color:white;\"></awards>\n      </v-dialog>\n      <v-dialog v-model=\"feesDialog\">\n        <fees style=\"background-color:white;\"></fees>\n      </v-dialog>\n  </v-app>\n\n    \n</template>\n\n<script>\nimport {auth, db, users} from '../firebaseconfig.js'\nimport {mapState} from 'vuex'\nimport addVehicle from './components/addVehicle.vue'\nimport addHook from './components/addHooks.vue'\nimport awards from './components/awards.vue'\nimport fees from './components/fees.vue'\n\nexport default {\n  name: 'App',\n\n  components: {\n    addVehicle,addHook, awards, fees\n  },\n\n  data: () => ({\n    drawer: false,\n    addMemberDialog: false,\n    addVehicleDialog: false,\n    addHookDialog:false,\n    awardsDialog: false,\n    feesDialog: false,\n    adminSpeedy: false,\n    group: null,\n    currentYear: '2020',\n    items : [\n      \n      {icon: '', text: 'About Us', link: '/about'},\n      {icon: '', text: 'Classes', link: '/vehicleClasses'},\n      {icon: '', text: 'Host Cities', link: '/host'},\n      \n    ]\n    //\n  }),\n  firestore() {\n    return {\n      //vehicleClasses: db.collection(\"vehicleClasses\")\n    }\n  },\n  methods: {\n    logout() {\n      auth.signOut().then(() => {\n        location.reload()\n        console.log('auth signed out and page reloaded')\n        this.$store.dispatch('clearData')\n        console.log('vuex cleared')\n      }).catch(err => {\n        console.log(err)\n      })\n    },\n    updateDialog(e) {\n      awardsDialog = !awardsDialog\n    }\n  },\n  watch: {\n    group () {\n      this.drawer = false\n    }\n  },\n  computed: {\n    ...mapState(['currentUser', 'userProfile', 'userRole']),\n    admin: function() { \n      let admin \n      if (this.$store.state.userProfile.role == 'admin') {return admin = true} else {return admin = false}\n      return admin\n      }\n  },\n  mounted() {\n    let fontawesomescript = document.createElement('script')\n    fontawesomescript.setAttribute('src', 'https://kit.fontawesome.com/be2cd14556.js')\n    document.head.appendChild(fontawesomescript)\n  }\n};\n</script>\n\n<style>\na {\n  color: #AF372F !important}\n\nbody::-webkit-scrollbar {\n  width: 0.5em;\n}\n \nbody::-webkit-scrollbar-track {\n  box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);\n}\n \nbody::-webkit-scrollbar-thumb {\n  background-color:red;\n  outline: 1px solid black;\n}\n\n\n@media print {\n  .no-print {\n    display: none !important\n  }\n}\n\n</style>"],"sourceRoot":"src"}]}